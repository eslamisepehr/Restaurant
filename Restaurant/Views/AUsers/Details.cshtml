@model Restaurant.Models.Entities.AspNetUser

@{
    ViewBag.Title = "جزییات";
}
<p class="navbar-left">
    @Html.ActionLink("ویرایش", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("برگشت به لیست کاربران", "Index")
</p>
<hr />
<div class="panel">
    <div class="panel-heading text-center border_Title_Basket">
        غذا ها
    </div>
    <div class="panel-body">
        <h2>جزییات</h2>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                ایمیل :
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Email)
            </dd>


            <dt>
                شماره تماس :
            </dt>

            <dd>
                @Html.DisplayFor(model => model.PhoneNumber)
            </dd>

            <dt>
                دسترسی
            </dt>

            <dd>
                @Html.DisplayFor(model => model.AspNetUserRole.RoleId)
            </dd>

        </dl>
    </div>
</div>

