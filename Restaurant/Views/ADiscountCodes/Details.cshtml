@model Restaurant.Models.Entities.DiscountCode

@{
    ViewBag.Title = "جزییات";
}
<p class="navbar-left">
    @Html.ActionLink("ویرایش", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("برگشت به لیست کد تخفیف", "Index")
</p>
<hr />
<div class="panel">
    <div class="panel-heading text-center border_Title_Basket">
        کد تخفیف
    </div>
    <div class="panel-body">
        <h2>جزییات</h2>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                کد :
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Code)
            </dd>

            <dt>
                درصد
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Persent)
            </dd>

        </dl>
    </div>
</div>


